{
  "compilerOptions": {
    "experimentalDecorators": true,
    "allowJs": true,
    "esModuleInterop": true,
    "outDir": "./dist/",
    "noImplicitAny": true,
    "sourceMap": true,
    "declaration": true,
    "module": "es6",
    "target": "es5",
    "moduleResolution": "node",
    "plugins": [
      { "name": "typescript-tslint-plugin" }
    ],
  },
  "exclude": [
    "**/*.svg"
  ]
}
